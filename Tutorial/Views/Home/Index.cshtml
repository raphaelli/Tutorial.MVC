@model IEnumerable<Tutorial.Model.Student>
    @{
        Layout = null;
    }

    <!DOCTYPE html>

    <html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>Index</title>
    </head>
    <body>
        <ul>
            @foreach (var a in Model)
            {
            <li>@a.stuId - @a.stuFirstName @a.stuLastName</li>       
            }
        </ul>
        
    </body>
</html>
